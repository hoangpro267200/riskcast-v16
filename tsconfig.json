{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "strict": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "**/*.min.js",
    "**/*.bundle.min.js",
    "**/*.bundle.min.css",
    "dist/**"
  ]
}




















